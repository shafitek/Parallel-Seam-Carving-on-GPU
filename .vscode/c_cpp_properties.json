{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/local/include",
                "/usr/local/cuda/include",
                "/usr/local/include/opencv4"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "path": [
                    "${workspaceFolder}/src"
                ]
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c18",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "configurationProvider": "go2sh.cmake-integration"
        }
    ],
    "version": 4
}